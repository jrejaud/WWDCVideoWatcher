{
  "name": "IDGAF",
  "manifest_version": 2,
  "version": "0.0.1",
  "description": "Watch Apple WWDC videos on Google Chrome",
  "content_scripts": [
    {
      "matches": ["https://developer.apple.com/videos/*"],
      "js": ["jquery-1.12.0.min.js","idgaf.js"],
      "run_at": "document_end"
    }
  ]
}
