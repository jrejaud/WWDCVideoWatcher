{
  "name": "WWDChrome",
  "manifest_version": 2,
  "version": "0.1.3",
  "description": "Watch Apple WWDC videos using Google Chrome",
  "content_scripts": [
    {
      "matches": ["https://developer.apple.com/videos/*"],
      "js": ["content_script.js"],
      "run_at": "document_end"
    }
  ],
  "icons": { "128": "icon128.png" }
}
